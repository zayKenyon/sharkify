{
	"name": "sharkify",
	"version": "1.0.0",
	"description": "",
	"main": "manifest.json",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "web-ext run --keep-profile-changes --profile-create-if-missing",
		"start:verbose": "web-ext run --keep-profile-changes --profile-create-if-missing --verbose",
		"build": "web-ext build --overwrite-dest",
		"sign": "dotenv -- web-ext sign --api-key=$WEB_EXT_API_KEY --api-secret=$WEB_EXT_API_SECRET --channel=$WEB_EXT_CHANNEL",
		"lint": "web-ext lint"
	},
	"private": true,
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"dotenv-cli": "^11.0.0",
		"web-ext": "^9.2.0"
	}
}
